public class Main {
    public static void main(String[] args) {
        var numeroWhile = -1;    
            
        while (numeroWhile <= 3){ 
            numeroWhile = numeroWhile +1;
        }
        do {
            System.out.println(numeroWhile);
            numeroWhile = numeroWhile +100;
        } while (numeroWhile <= 3);
    }
}
